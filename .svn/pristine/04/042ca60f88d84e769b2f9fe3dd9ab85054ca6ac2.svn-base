/* pages/shibor/shibor.wxss */
page{
  background:#fff;
}
.container{
   width:94%;
   padding:0 3%;
}
.row{
  width:100%;
  height:70rpx;
  border-bottom:#e8e8e8 solid 1px;
}
.row text{
  float:left;
  width:25%;
  height:70rpx;
  line-height:70rpx;
  font-size:30rpx;
  text-align:center;
}
.row view{
  float:left;
  width:25%;
  height:70rpx;
  line-height:70rpx;
  font-size:30rpx;
  text-align:center;
}
.row view image{
  width:20rpx;
  height:23rpx;
}