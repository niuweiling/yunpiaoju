.details-jg {
	margin-top: 20rpx;
	background: #fff;
	position: relative
}

.jg-top {
	height: 88rpx;
	line-height: 88rpx;
	padding: 0 20rpx;
}

.d4 {
	font-size: 30rpx;
	color: #333333;
	margin-left: 40rpx;
}

.d5 {
	width: 15rpx;
	height: 32rpx;
	transform: rotate(90deg);
	margin-top: 35rpx;
}

.d5.cur {
	transform: rotate(180deg);
}

.jg-content {
	padding: 0 20rpx 30rpx;
	box-sizing: border-box;
	border-top: #e6e6e6 solid 1px;
	font-size: 30rpx;
}

.jg-content text {
	display: block;
}

.rotateRight {
	transform: rotate(270deg)
}

.jg-content .jbxx:last-child {
	border-bottom: none
}

.jbxx {
	padding: 0 0 20rpx;
	border-bottom: #e6e6e6 solid 1px;
}

.jbxx1 {
	margin-top: 20rpx;
	color: #333333;
}

.jbxx2 {
	color: #666;
}

.jbxx3 {
	font-size: 26rpx;
	color: #333333;
	margin-top: 20rpx;
}

.jbxx text {
	display: block;
}

.flex {
	display: flex;
}

.one .one-list:first-child {
	border-right: #e6e6e6 solid 1px;
	margin-right: 20rpx;
}

.one-list {
	width: 50%;
}

.a1 {
	margin-top: 20rpx;
}

.a2 {
	margin-top: 0
}
