.list{
  background: #fff;
  width: 600rpx;
  height: 80rpx;
  line-height: 80rpx;
  padding:0 20rpx;
  box-sizing: border-box;
  margin: 0 auto;
  border-radius: 10rpx;
  position: relative
}
.a1{
  margin-top: 80rpx;
}
.a2{
  margin-top: 40rpx;
}
.list input{
  display: block;
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 28rpx;
}
.password{
  width: 36rpx;
  height: 36rpx;
  float: right;
  margin-top: 26rpx;
}
.dl{
  background: #8797ED;
  width: 600rpx;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 28rpx;
  color: #fff;
  margin-top: 72rpx;
}
.text{
  font-size: 32rpx;
  color: #888888;
  text-align: center;
  margin-top: 54rpx;
}
#width{
  width: 90%
}