page {
	background: #fff;
}

.content {
	padding: 20rpx 30rpx 0;
	box-sizing: border-box;
}

.search {
	background-color: #E5E5E5;
	width: 100%;
	height: 70rpx;
	line-height: 70rpx;
	border-radius: 10rpx;
	font-size: 24rpx;
	color: #757375;
	text-align: center;
	padding-left: 20rpx;
	box-sizing: border-box;
	position: relative
}

.icon-search {
	display: block;
	width: 32rpx;
	height: 32rpx;
	position: absolute;
	left: 46rpx;
	top: 40rpx;
	z-index: 22
}

.nokey {
	display: block;
	width: 200rpx;
	height: 124rpx;
	margin: 250rpx auto 0;
}

.nofind {
	display: block;
	font-size: 28rpx;
	color: #757375;
	text-align: center;
	margin-top: 40rpx;
}

.key-btn {
	display: block;
	width: 284rpx;
	height: 70rpx;
	line-height: 70rpx;
	background: #8797ED;
	color: #fff;
	font-size: 28rpx;
	margin-top: 40rpx;
}
