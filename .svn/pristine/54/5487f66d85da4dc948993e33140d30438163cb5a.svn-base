.top{
  width: 100%;
  height: 88rpx;
  line-height: 88rpx;
  background: #fff;
  position: fixed;
  top: 0;
  font-size: 32rpx;
  color: #101010;
  text-align: center;
  z-index: 2
}
.top-btn{
  width: 40rpx;
  height: 20rpx;
}
.top-btn.rotateRight{  
  transform: rotate(180deg); 
} 
.top-content{
  width:406rpx;
  height:350rpx;
  background:rgba(0, 0, 0, 0.7);
  position:fixed;
  top:125rpx;
  left:50%;
  transform:translateX(-50%);
  z-index:100;
  box-sizing:border-box;

}
.top-content::after{
  content: "";
  position: absolute;
  left: 50%;
  top: -40rpx;
  width: 0;
  height: 0;
  border-width: 20rpx;
  border-style: solid;
  border-color: transparent transparent black transparent;
  transform: translateX(-50%)
}
.top-list{
  width: 100%;
  height: 70rpx;
  line-height: 70rpx;
  border-bottom: solid #e6e6e6 1px;
  padding-left: 20rpx;
  box-sizing: border-box;
  font-size: 24rpx;
  color: #fff;
  display: flex;
}
.yuan{
  width: 20rpx;
  height:20rpx;
  border-radius: 50%;
  margin-right: 20rpx;
  margin-top: 30rpx;
}
.red{
  background: #DE4A2A;
}
.green{
  background: #8BC34A;
}
.blue{
  background: #0094FF;
}
.yellow{
  background: #D9D341;
}
.top-content .top-list:last-child{
  border-bottom: 0
}
.search{
  display: block;
  width: 40rpx;
  height: 40rpx;
  position: absolute;
  right: 30rpx;
  top: 25rpx;
  z-index: 10
}
page{
