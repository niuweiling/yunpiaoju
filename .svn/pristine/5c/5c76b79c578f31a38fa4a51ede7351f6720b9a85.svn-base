/* pages/car/cal.wxss */
.container {
 
  padding: 8rpx 0;
  background: #e6e6e6;
  font-size:14px;
  font-family: "微软雅黑";
  box-sizing: border-box;
} 
.tc{
  text-align:center;
}
.flex-row{
  width:100%;
  height:60rpx;
  line-height:60rpx;
  /* margin-bottom:20rpx; */
  overflow:hidden;
}
.flex-row-fl3{
  width:32%;
  float:left;
  height:120rpx;
  line-height:60rpx;
 
}
.flex-row text{
  padding-left:30rpx;
  float:left;
  height:60rpx;
  line-height:60rpx;
  width:230rpx;
  color:#222222;
}
.cal_con .flex-row text{
  padding-left:0rpx;
  float:left;
  height:60rpx;
  line-height:60rpx;
  width:230rpx;
  color:#333333;
}
.flex-row input{
  display: inline-block;
  width:420rpx;
  height:60rpx;
  line-height:60rpx;
  background:#e6e6e6;
  color:#808080;
  border-radius:10rpx;
  -webkit-border-radius:10rpx;
  padding-left:10rpx;
  z-index:1;
}
.flex-row input.stxt{
  width:160rpx;
}
.flex-row button{
  display: inline-block;
  padding:0 5rpx;
  width:110rpx;
  line-height:60rpx;
  margin-left:20rpx;
  margin-right:0;
  background:#cecece;
  color:#fff;
}
.flex-row-fl3 input{
  width:200rpx;
}
.flex-row button.true{
 background:#7ecef4;
}
.flex-row button.medium{
  width:324rpx;
  line-height:60rpx;
  margin-left:25rpx;
  margin-right:0;
  font-size:14px;
}
.flex-row input.mtxt{
  width:225rpx;
  margin-right:20rpx;
}
.flex-row input.greybg{
  background:#888888;
  color:#fff;
}
.flex-row button.orange{
  margin-left:0;
  width:170rpx;
  background:#e45a5c;
  font-size:14px;
}
.v-mode{
  display: inline-block;
  width:420rpx;
  height:60rpx;
  line-height:60rpx;
  background:#fff;
