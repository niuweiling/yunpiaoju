page{
  background: #fff;
}
.content{
  padding: 26rpx 30rpx 0;
  box-sizing: border-box;
  background: #fff;
}
.search{
  background-color: #e5e5e5;
  width: 100%;
  height: 70rpx;
  line-height: 70rpx;
  border-radius: 10rpx;
  font-size: 24rpx;
  color: #BEC2C9;
  padding-left: 30rpx;
  box-sizing: border-box;
}
.icon-search{
  float: left;
  width: 28rpx;
  height: 28rpx;
  margin-top: 20rpx;
  margin-right: 20rpx
}
swiper{
   margin-top: 40rpx;
   height: 210rpx;
   }
swiper image{ 
  width: 100%;
  height: 100%;
  }
.btn-area{ 
  display: flex;
  flex-direction: row;
  margin-top: 20rpx;
  padding-bottom: 10rpx;
  }
.btn-area view{
   width: 25%;
   font-size: 24rpx; 
   color: #757375;
   text-align: center
   }
.icon-nav{
  display: block;
  width: 98rpx;
  height: 98rpx;
  margin: 0 auto;
}
.i1{
  font-size: 40rpx;
  color: #101010;
  line-height: 58rpx;
}
.i2{ 
  height: 40rpx;
  background: #eee;
}
.i3{
  font-size: 28rpx;
  color: #757375;
  line-height: 40rpx;
  height: 40rpx;
}
.circle{
  width: 240rpx;
  height: 240rpx;
  margin: 60rpx auto 0;
  position: relative;


}
.one{
  background: rgba(135, 151, 237, 0.6);
  width: 240rpx;
  height: 240rpx;
  border-radius: 50%;
  position: absolute;
  left: 0;
  top: 0;
  z-index: 1
  
}
.two{
  background: rgba(135, 151, 237, 0.8);
  width: 200rpx;
  height: 200rpx;
  border-radius: 50%;
  position: absolute;
  left: 20rpx;
  top: 20rpx;
  z-index: 2
}
.icon-shibie{
  display: block;
  width: 160rpx;
  height: 160rpx;
  position: absolute;
  left: 40rpx;
  top: 40rpx;
  z-index: 3
}
@keyframes lamp-one{
			from{ opacity: 0.6; transform: scale(0.7);}
			to{ opacity: 0; transform: scale(1);}
		}
@keyframes lamp-two{
  from{ opacity: 0.8; transform: scale(0.8);}
  to{ opacity: 0; transform: scale(1);}
}
.one{
  animation-name: lamp-one;
  animation-duration: 3s;
  animation-iteration-count: infinite;
}
.two{
  animation-name: lamp-two;
  animation-duration: 3s;
  animation-iteration-count: infinite;
}