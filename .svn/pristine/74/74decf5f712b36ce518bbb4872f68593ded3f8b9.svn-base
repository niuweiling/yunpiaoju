.fj{
  margin-top: 100rpx;
}
.fixed-top{
  background: #fff;
  position: fixed;
  width: 91%;
  height: 100rpx;
  top: 0rpx;
  padding-top:26rpx;
 box-sizing: border-box;
}
page{
  background: #fff;
  position: relative
}
.clear{ clear: both; height: 0; overflow: hidden;}
.fl{ float: left}
.fr{ float: right}
.content{
  padding: 26rpx 30rpx 0;
  box-sizing: border-box;
}
.wid{
  height: 70rpx;
  position: relative;
}
.search{
  background-color: #fff;
  width: 100%;
  height: 70rpx;
  line-height: 70rpx;
  border-radius: 10rpx;
  font-size: 24rpx;
  color: #757375;
  text-align: left;
  padding-left: 20rpx;
  box-sizing: border-box;
  position: relative
}
.icon-search{
  display: block;
  width: 28rpx;
  height: 28rpx;
  position: absolute;
  left: 26rpx;
  top: 20rpx;
  z-index: 22
}
.search-close{
  display: block;
  width: 28rpx;
  height: 28rpx;
  position: absolute;

  right: 40rpx;
  top: 20rpx;

  z-index: 33
}
.hot{
  font-size: 30rpx;
  color: #757375;
  margin-top: 40rpx;
}
.hot-item{
  float: left;
  background: #e6e6e6;
  font-size: 24rpx;
  padding: 10rpx 20rpx;
  color: #757375;
  margin-top: 20rpx;
  margin-right: 20rpx;
  border-radius: 10rpx;
}
.search-list{
  height: 70rpx;
  line-height: 70rpx;
  border-bottom: #e6e6e6 solid 1px;
  font-size: 24rpx;
  color: #757375;
  text-align: center;
  margin-top: 20rpx
}
.search-time{
  width: 24rpx;
  height: 24rpx;
  margin-top: 22rpx;
}
.search-text{
  margin-left: 30rpx;
}
#search-btn{
  background: none;
  margin-top: 20rpx;
  color: #757375
}
button::after{
  border: none
}



