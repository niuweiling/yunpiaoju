/* pages/card/carddetail/carddetail.wxss */
page{
  background: #fff;
}
.container{
  width:94%;
  padding:0 3%;
}
.edit-list{
  width:100%;
  height:112rpx;
  line-height:112rpx;
  font-size:30rpx;
  color:#333;
  padding:10rpx 0;
  background: #fff;
  border-bottom:#e8e8e8 solid 1px;
}
.list-first{
  width:140rpx;

}
.edit-right-bar{
 
   height:112rpx;
}
.head-portrait{
  float:right;
  width:112rpx;
  height:112rpx;
  border-radius:50%;
  -webkit-border-radius:50%;
  margin:0 20rpx;
}
.edit-list2{
  height:70rpx;
  line-height:70rpx;
}
.edit-user{
  height:70rpx;
  line-height:70rpx;
  color:#666;
}
.edit-user2{
  line-height:56rpx;
  color:#666;
  width:550rpx;
}
.edit-list3{
  min-height:160rpx;
  padding:10rpx 0;
}
.arrow{
  width:10rpx;
  height:20rpx;
  margin-top:43rpx;
}

.clear{
  clear: both;
  height: 0;
  overflow: hidden;
}

.edit-list4{
  margin-top:5px;
  display: flex;
  width: 100%;
  font-size:30rpx;
  color:#333;
  padding:10rpx 0;
  background: #fff;
  border-bottom: #e8e8e8 solid 1px;
}
 .edit-list4 textarea{
   margin-top: 0;
   padding-top: 0;
} 
.keepBtn{
  background: #518cee;
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
  width: 100%;
  height: 90rpx;
  line-height: 90rpx;
  font-size: 28rpx;
  color: #fff;
  border-radius: 0;

}