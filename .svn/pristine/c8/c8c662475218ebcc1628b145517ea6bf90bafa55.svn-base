.img-success{
  display: block;
  width: 90rpx;
  height: 90rpx;
  position: absolute;
  right: 40rpx;
  top: 20rpx;
}
.top{
  width: 100%;
  height: 90rpx;
  line-height: 90rpx;
  background: #fff;
  position: fixed;
  top: 0;
  font-size: 32rpx;
  color: #101010;
  text-align: center;
  z-index: 2
}
.top-btn{
  width: 20rpx;
  height: 10rpx;
}
.top-btn.rotateRight{  
  transform: rotate(180deg); 
} 
.top-content{
  width:406rpx;
  height:410rpx;
  background:rgba(81, 140, 238, 0.8);
  position:fixed;
  top:100rpx;
  left:50%;
  transform:translateX(-50%);
  z-index:100;
  box-sizing:border-box;
  border-radius: 2%;
  /* opacity: 0.8; */
  padding-left: 20rpx;
  padding-right: 20rpx;
}
.top-content::after{
  content: "";
  position: absolute;
  left: 50%;
  top: -20rpx;
  width: 0;
  height: 0;
  border-width: 10rpx;
  border-style: solid;
  border-color: transparent transparent #518cee transparent;
  transform: translateX(-50%)
}
.top-list{
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  border-bottom: solid #e6e6e6 1px;
  padding-left: 20rpx;
  box-sizing: border-box;
  font-size: 24rpx;
  color: #fff;
  display: flex;
}
.yuan{
  width: 20rpx;
  height:20rpx;
  border-radius: 50%;
  margin-right: 20rpx;
  margin-top: 30rpx;
}
.red{
  background: #DE4A2A;
}
.green{
  background: #8BC34A;
}
.blue{
  background: #0094FF;
}
.yellow{
  background: #D9D341;
}
.top-content .top-list:last-child{
  border-bottom: 0
}
.search{
  display: block;
  width: 28rpx;
  height: 28rpx;
  position: absolute;
  right: 30rpx;
  top: 30rpx;
  z-index: 10
}
