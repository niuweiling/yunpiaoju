.hpsc{
  width: 75%;
  border-left: solid 1px #cacaca;
  padding-bottom: 30rpx;
}
.hpsc-list{
  float: right;
  width: 500rpx;
  height: auto;
  background: #fff;
  margin-top: 30rpx;
  padding: 10rpx;
  box-sizing: border-box;
  position: relative;
}
.hpsc-list text{
  display: block;
  margin-top: 5rpx;
}
.hpsc-list::after {
  content: "";
  position: absolute;
  left: -40rpx;
  top: 20rpx;
  width: 0;
  height: 0;
  border-width: 20rpx;
  border-style: solid;
  border-color: transparent white transparent transparent;
}
